{
  "buildCommand": "prisma generate && next build",
  "installCommand": "yarn install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/refresh-token",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/sync",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/sync/cleanup",
      "schedule": "0 9 * * *"
    }
  ]
}
